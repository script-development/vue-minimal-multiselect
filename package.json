{
    "name": "@script-development/vue-services",
    "version": "0.2.3",
    "description": "Services for Vue SPA App",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "typings": "types/index.d.ts",
    "files": [
        "dist",
        "types",
        "src"
    ],
    "scripts": {
        "test": "nyc mocha --recursive --require @babel/register",
        "build": "rollup -c scripts/config.js",
        "dev": "vue-cli-service serve --open",
        "watch": "rollup -c scripts/config.js --watch",
        "pub": "npm version patch && npm publish",
        "docs": "typedoc",
        "lint": "eslint ./src",
        "mutate": "stryker run"
    },
    "author": "Script",
    "license": "MIT",
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "core-js": "^3.6.5",
        "vue-template-compiler": "^2.6.11",
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.5",
        "@babel/register": "^7.11.5",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@stryker-mutator/core": "^3.3.1",
        "@stryker-mutator/javascript-mutator": "^3.3.1",
        "@stryker-mutator/mocha-runner": "^3.3.1",
        "eslint": "^7.7.0",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^4.2.5",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.0.0",
        "rollup": "^2.26.11",
        "typedoc": "^0.19.1",
        "typescript": "^4.0.2",
        "vue": "^2.6.12"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|json)\""
        }
    },
    "nyc": {
        "reporter": [
            "lcov",
            "text"
        ]
    }
}
